{% if my_user_id != contributor_id %}
{% if following %}
<button class="button size-2 unfollow"onclick=" updateFollowUserStatus('{{ contributor_id }}', 0, this)">Following</button>
{% else %}
<button class="button size-2 follow"onclick=" updateFollowUserStatus('{{ contributor_id }}', 1, this)">Follow</button>
{% endif %}
{% endif %}


