<div class="common_table_header">

	<div style="width:10%;">&nbsp;</div>
	<div style="width:50%;text-align:left;">{{ field1 }}</div>
	<div style="width:20%;">Access Control</div>
	<div style="width:10%;">&nbsp;</div>
</div>


  {% for item in items %}
   {% if  item['folder_name'] %}
     <div id="row_{{ loop.index0 + 1 }} " class="DivContentTable_item  {{ loop.index0 is odd ? 'odd' : 'even' }}">
               <div style="width:9%;float:left;display:block;text-align:center;">{{ loop.index0 + 1 }}</div>
               <div style="width:58%;float:left;display:block;">{{ item['folder_name'] }}</div>
               <div style="width:8%;float:left;display:block;text-align:center;"><a href="#add-record" onclick="getAddGroupMemberUI('fileshare',29);">Public</a></div>
               <div style="width:20%;float:right;display:block;margin-left:20px;">
                    <button>Edit </button>
                    <button onClick="module_item_delete('fileshare_folder',{{ item['folder_id'] }});">Delete </button></div>
                </div>
   {% endif %}
 {% endfor %}